<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.show.showticketingservice.mapper.SeatInfoMapper">

    <insert id="insertSeatInfo" parameterType="com.show.showticketingservice.model.performance.SeatRequest">
        INSERT INTO seat(perfTimeId, hallId, colNum, rowNum, reserved)
        VALUE
        <foreach collection="seatRequests" item="seatRequest" separator=",">
            (
            #{seatRequest.perfTimeId},
            #{seatRequest.hallId},
            #{seatRequest.colNum},
            #{seatRequest.rowNum},
            #{seatRequest.reserved}
            )
        </foreach>
    </insert>

</mapper>
